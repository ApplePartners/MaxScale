[maxscale]
threads=###threads###

###server###

[MariaDB-Monitor]
type=monitor
module=mysqlmon
servers=###server_line###
user=maxskysql
password=skysql
monitor_interval=1000

[Matcher]
type=filter
module=namedserverfilter
match01=DELETE
target01=server1

[Schemarouter]
type=service
router=schemarouter
servers=###server_line###
user=maxskysql
password=skysql
ignore_tables_regex=.*
filters=Matcher

[Schemarouter-Listener]
type=listener
service=Schemarouter
protocol=MariaDBClient
port=4006
