[maxscale]
threads=###threads###

###server###

[MySQL-Monitor]
type=monitor
module=mysqlmon
servers=###server_line###
user=maxskysql
password=skysql

[Sharding-Router]
type=service
router=schemarouter
servers=###server_line###
user=maxskysql
password=skysql
ignore_tables_regex=.*

[Sharding-Listener]
type=listener
service=Sharding-Router
protocol=MySQLClient
port=4006
