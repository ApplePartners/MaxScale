[maxscale]
threads=###threads###
key_manager=file
file.keyfile=/tmp/encryption.key

###server###

[mariadb-cluster]
type=monitor
module=mariadbmon
servers=###server_line###
user=maxskysql
password=skysql
monitor_interval=1000ms
auto_failover=true

[pinloki-service]
type=service
router=binlogrouter
user=maxskysql
password=skysql
cluster=mariadb-cluster
encryption_key_id=1
select_master=true

[pinloki-listener]
type=listener
service=pinloki-service
protocol=mariadbclient
port=4006
