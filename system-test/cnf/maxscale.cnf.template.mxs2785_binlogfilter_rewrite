[maxscale]
threads=###threads###
log_info=1

###server###

[MySQL-Monitor]
type=monitor
module=mysqlmon
servers=###server_line###
user=maxskysql
password=skysql
monitor_interval=2000ms

[blf]
type=filter
module=binlogfilter
rewrite_src=test_[a-z0-9]*
rewrite_dest=$0_rewritten

[Read-Connection-Router]
type=service
router=readconnroute
router_options=master
servers=###server_line###
user=maxskysql
password=skysql
filters=blf

[Read-Connection-Listener]
type=listener
service=Read-Connection-Router
protocol=MySQLClient
port=4008
