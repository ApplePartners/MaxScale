[maxscale]
threads=###threads###
log_warning=1

[server1]
type=server
address=###node_server_IP_1###
port=###node_server_port_1###
protocol=MySQLBackend

[MySQL-Monitor]
type=monitor
module=mysqlmon
servers=server1
user=maxskysql
password=skysql
monitor_interval=1000ms

[RWS]
type=service
router=readwritesplit
servers=server1
user=maxskysql
password=skysql

[RWS-Listener]
type=listener
service=RWS
protocol=MySQLClient
port=4006

[Cache-redis]
type=filter
module=cache
storage=storage_redis
storage_options="server=127.0.0.1"
enabled=true
timeout=2s
debug=31

[RWS-Cache-redis]
type=service
router=readwritesplit
servers=server1
user=maxskysql
password=skysql
filters=Cache-redis

[RWS-Cache-redis-Listener]
type=listener
service=RWS-Cache-redis
protocol=MySQLClient
port=4007

[Cache-memcached]
type=filter
module=cache
storage=storage_memcached
storage_options="server=127.0.0.1"
enabled=true
timeout=2s

[RWS-Cache-memcached]
type=service
router=readwritesplit
servers=server1
user=maxskysql
password=skysql
filters=Cache-memcached

[RWS-Cache-memcached-Listener]
type=listener
service=RWS-Cache-memcached
protocol=MySQLClient
port=4008
