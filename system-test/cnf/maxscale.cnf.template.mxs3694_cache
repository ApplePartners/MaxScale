[maxscale]
threads=###threads###

[server1]
type=server
address=###node_server_IP_1###
port=###node_server_port_1###
protocol=MySQLBackend

[MySQL-Monitor]
type=monitor
module=mysqlmon
servers=server1
user=maxskysql
password=skysql
monitor_interval=1000

[Cache]
type=filter
module=cache
soft_ttl=2s
hard_ttl=4s
cached_data=shared
invalidate=current
storage=storage_inmemory
enabled=true

[RWS]
type=service
router=readwritesplit
user=maxskysql
password=skysql
servers=server1
filters=Cache

[RWS-Listener]
type=listener
service=RWS
protocol=mariadbclient
port=4006
